<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.Controllers.LineController">
   <children>
      <Button fx:id="exitLineChart" layoutX="284.0" layoutY="351.0" mnemonicParsing="false" onAction="#exitChart" text="ОК" />
      <LineChart fx:id="lineChart" layoutX="102.0" layoutY="45.0" prefHeight="287.0" prefWidth="395.0" title="Динамика результатов расчетов">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
